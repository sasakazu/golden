  pod 'Firebase/Auth'
  pod 'FirebaseUI'
  

  target 'goldenTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'goldenUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
